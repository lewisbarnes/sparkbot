{
	"compilerOptions": {
			"target": "ESNext",
			"module": "CommonJS",
			"rootDir": "bot/src",
			"outDir": "./dist/",
			"strict": true,
			"moduleResolution": "node",
			"importHelpers": true,
			"experimentalDecorators": true,
			"esModuleInterop": true,
			"skipLibCheck": true,
			"allowSyntheticDefaultImports": true,
			"resolveJsonModule": true,
			"forceConsistentCasingInFileNames": true,
			"removeComments": true,
			"typeRoots": [
					"node_modules/@types"
			],
			"sourceMap": false,
			"baseUrl": "./"
	},
	"include": [
			"./**/*.ts"
, "webpanel/src/components/command.tsx"	],
	"exclude": [
			"node_modules",
			"dist"
	],
}